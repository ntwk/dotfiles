      <mousebind button="Down" action="Click">
        <action name="DesktopRight">
          <dialog>no</dialog>
          <wrap>no</wrap>
        </action>
      </mousebind>
      <mousebind button="A-Up" action="Click">
        <action name="DesktopLeft">
          <dialog>no</dialog>
          <wrap>no</wrap>
        </action>
      </mousebind>
